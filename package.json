{
  "name": "epickup-admin-dashboard",
  "version": "1.0.0",
  "description": "EPickup Real-Time Admin Dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test:endpoints": "node test-endpoints.js",
    "analyze:bundle": "npm run build && node analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "setup:admin": "node setup-admin-user.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/x-data-grid": "^6.18.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/react-router-dom": "^5.3.3",
    "dotenv": "^17.2.3",
    "firebase": "^12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.4",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/parse-json": "^4.0.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "terser": "^5.44.0",
    "typescript": "^5.3.3",
    "vite": "^7.1.4"
  }
}
